{
  "name": "leantime-estimateimport",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/itk-dev/leantime-estimateimport",
  "license": "MIT",
  "devDependencies": {
    "markdownlint-cli": "^0.39.0",
    "prettier": "^3.2.4"
  },
    "scripts": {
        "coding-standards-check/prettier": "prettier assets --check",
        "coding-standards-check/markdownlint": "markdownlint --ignore LICENSE.md '*.md' docs/",
        "coding-standards-check": "yarn coding-standards-check/prettier; yarn coding-standards-check/markdownlint",
        "coding-standards-apply/prettier": "prettier assets --write",
        "coding-standards-apply/markdownlint": "markdownlint --ignore LICENSE.md '*.md' docs/ --fix",
        "coding-standards-apply": "yarn coding-standards-apply/prettier; yarn coding-standards-apply/markdownlint"
    }
}
